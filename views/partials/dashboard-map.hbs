<div class="box" id="map" style="height: 400px;"></div>
<script>
  const map = L.map('map').setView([52.818243, -7.826639], 7);
  L.tileLayer('https://tile.openstreetmap.org/{z}/{x}/{y}.png', {
    maxZoom: 19,
    attribution: 'Â© OpenStreetMap'
}).addTo(map);

{{#each stations}}
  L.marker([{{latitude}}, {{longitude}}]).addTo(map)
    .bindPopup("Station: {{name}}<br>Latitude: {{latitude}}<br>Longitude: {{longitude}}");
  {{/each}}
</script>